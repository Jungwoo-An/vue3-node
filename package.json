{
  "name": "vue3-node",
  "version": "1.0.0",
  "description": "Vue3 (SFC) execution environment for node.js",
  "keywords": [
    "vue",
    "vue3",
    "vue3-node",
    "vue-node",
    "node-vue",
    "vue-require",
    "require-vue"
  ],
  "scripts": {
    "build": "rollup --config --environment NODE_ENV:production",
    "e2e": "yarn build && TS_NODE_PROJECT=./tsconfig.e2e.json TS_NODE_FILES=true mocha --require ./dist/vue3-node.js --require ts-node/register ./e2e/specs/**/*spec.ts"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jungwoo-An/vue3-node.git"
  },
  "bugs": {
    "url": "https://github.com/Jungwoo-An/vue3-node/issues"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@vue/test-utils": "^2.0.0-beta.7",
    "chai": "^4.2.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.1.3",
    "mocha-jsdom": "^2.0.0",
    "rollup": "^2.32.1",
    "rollup-plugin-typescript2": "^0.28.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": "*"
  }
}
